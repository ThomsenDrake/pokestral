[flake8]
max-line-length = 88
extend-ignore = E203, E266, E501, W503, F403, F401
max-complexity = 18
select = B,C,E,F,W,T4,B9
exclude =
    .git,
    __pycache__,
    *.pyc,
    .pytest_cache,
    .tox,
    venv,
    .venv,
    env,
    .env,
    node_modules,
    build,
    dist,
    *.egg-info,
    .mypy_cache,
    .dmypy.json,
    dmpy.json